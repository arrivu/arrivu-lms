<% else %>
<% @body_classes << "gradebook2 full-width"
  js_bundle :course_library,:course_comments,:bootstrap
  jammit_css :course_box,:topics_listing, :bootstrap_and_responsive
%>

<div id="course_listing_banner">
<div class="container">
  <h1 id="discourse_course_title">Discover Courses</h1>
</div>
</div>
